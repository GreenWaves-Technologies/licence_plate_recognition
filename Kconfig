menu "Licence Plate Recognition APP menu"

    comment "Options"

    choice APP_MODE
        prompt "Application Mode"
        config APP
            bool "Complete application"
        config SSD_ONLY
            bool "Only Detector"
        config LPR_ONLY
            bool "Only LPR"

    endchoice

    choice NNTOOL_SCRIPT
        prompt "NNTool Script"
        config MODEL_NE16
            bool "NE16 Model"
            select CNN_NE16

        config MODEL_SQ8
            bool "Int8 Model"
            select CNN_SQ8

    endchoice

endmenu
source "$(GAP_SDK_HOME)/utils/kconfig/Kconfig"
